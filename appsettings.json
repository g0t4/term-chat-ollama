{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Https": {
        "Url": "https://0.0.0.0:5000",
        "Certificate": {
          "Path": "./tls/cert.pem",
          "KeyPath": "./tls/key.pem"
        }
      }
    }
  }
}
